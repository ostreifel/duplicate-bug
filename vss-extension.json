{
    "manifestVersion": 1,
    "id": "duplicate-bug",
    "version": "1.0.19",
    "name": "Duplicate Bug",
    "scopes": [ "vso.work", "vso.work_write" ],
    "description": "Detect duplication as bugs are filed.",
    "publisher": "ottostreifel",
    "icons": {
        "default": "img/dupBug.png"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "tags": [
        "Work Item",
        "group",
        "bug",
        "duplicate",
        "related"
    ],
    "content": {
        "details": {
            "path": "details.md"
        }
    },
    "branding": {
        "color": "rgb(220, 235, 252)",
        "theme": "light"
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/ostreifel/duplicate-bug"
    },
    "files": [
        {
            "path": "img",
            "addressable": true
        },
        {
            "path": "dist",
            "addressable": true
        },
        {
            "path": "duplicatesGroup.html",
            "addressable": true
        }
    ],
    "categories": [
        "Plan and track"
    ],
    "contributions": [
        {
            "id": "duplicates-group",
            "type": "ms.vss-work-web.work-item-form-group",
            "description": "",
            "targets": [
                "ms.vss-work-web.work-item-form"
            ],
            "properties": {
                "name": "Similar work",
                "uri": "duplicatesGroup.html",
                "height": 110
            }
        }
    ]
}
